		int arr[] = {50, 90, 33, 10, 9, 6};
		partition(arr, 0, arr.length - 1);
		System.out.println(Arrays.toString(arr));

	}
	
	public static void partition(int arr[], int start, int end) {
		
		int pivot = arr[end];
		
		int pi = start - 1;
		
		for(int i = start; i < end; ++i) {
			if(arr[i] < pivot) {
				++i;
				int temp = arr[i];
				arr[i] = arr[pi];
				arr[pi] = arr[i];
			}
		}
		
		int temp = arr[end];
		arr[end] = arr[pi + 1];
		arr[pi + 1] = temp;
	}
	


}
